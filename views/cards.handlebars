{{#each card.segments}}

     <div class="row">
        <div class="col s12 m7">
          <div class="card">
            <div class="card-image">
              <img src="http://centerforcreativemedia.com/wp-content/uploads/2016/02/up_movie_balloons_house-1440x900.jpg">
              <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">record_voice_over</i></a>
            </div>
            <div class="card-content">
              <p>{{this}}</p>
            </div>
          </div>
        </div>
      </div>

{{/each}}